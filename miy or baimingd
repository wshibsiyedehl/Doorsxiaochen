local OrionLib = loadstring(game:HttpGet("https://pastebin.com/raw/tewnkD8f"))()

 

OrionLib:MakeNotification({

  Name = "陈脚本",

  Content = "欢迎使用",

  Image = "rbxassetid://13133167047",

  Time = 10

})

wait(3)

local Window = OrionLib:MakeWindow({Name = "陈脚本)密钥系统", HidePremium = false, SaveConfig = false, IntroText = "密钥系统", ConfigFolder = "密钥系统"})

 

local Key = Window:MakeTab({

  Name = "密钥",

  Icon = "rbxassetid://13169958031",

  PremiumOnly = false

})

 

playername=tostring(game.Players.LocalPlayer.Character);

 

function rightkey()

  OrionLib:MakeNotification({

    Name = "你已添白名单",

    Content = "等待",

    Image = "rbxassetid://13133167047",

    Time = 10

  })

end

 

function load()

  rightkey()

  loadstring(game:HttpGet("https://pastebin.com/raw/Hbiz2Nrc"))()

end

 

function wrong002()

  OrionLib:MakeNotification({

    Name = "你没有白名单",

    Content = "请自行联系作者添加",

    Image = "rbxassetid://13133167047",

    Time = 10

  })

end

 

function wrong()

  OrionLib:MakeNotification({

    Name = "已获取白名单",

    Content = "你的密钥错误",

    Image = "rbxassetid://13133167047",

    Time = 8

  })

end

 

--密钥和用户名区

 

function yhm()

  if playername == "Chinese1233211234567" then

    if Input == "114514" then

      load()

     else

      wrong()

    end

   else

    yhm1()

  end

end

function yhm1()

  if playername == "baiyede" then

    if Input == "坤坤太美" then

      load()

     else

      wrong()

    end

   else

    yhm2()

  end

end

 

