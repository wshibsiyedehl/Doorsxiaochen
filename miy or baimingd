--验证区

Key:AddParagraph("陈脚本永久免费","")

Key:AddParagraph("用户名:"..playername,"")

Key:AddParagraph("一人一密钥","")

 

Key:AddTextbox({

  Name = "请输入密钥",

  Default = "",

  TextDisappear = true,

  Callback = function(Value)

    Input = Value

  end

})

 

Key:AddButton({

  Name = "确定密钥",

  Callback = function()

    yhm()

  end

})

